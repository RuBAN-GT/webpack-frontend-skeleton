{
  "plugins": [
    ["module-resolver", {
      "root": ["./app"],
      "alias": {
        "assets": "./app/assets",
        "helpers": "./app/helpers",
        "javascripts": "./app/javascripts",
        "root": "./app"
      }
    }],
  ],
  "presets": [
    [ "es2015", { "modules": false } ],
    [ "env", { "modules": false } ]
  ]
}
